<p id="notice"><%= notice %></p>

<h1>Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Calendarid</th>
      <th>Title</th>
      <th>Category</th>
      <th>Duedateclass</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.calendarId %></td>
        <td><%= schedule.title %></td>
        <td><%= schedule.category %></td>
        <td><%= schedule.dueDateClass %></td>
        <td><%= schedule.start %></td>
        <td><%= schedule.end %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div id="", data: {schedules: @schedules.to_json}></div>

  <div>
<%= tag.div nil, id: "arrow", data: {schedules: @schedules.to_json} %>
    </div>

<%= link_to 'New Schedule', new_schedule_path %>
